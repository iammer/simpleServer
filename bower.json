{
  "name": "simpleServer",
  "description": "Simple test server",
  "version": "0.0.1",
  "dependencies": {
    "jquery": "latest",
    "foundation": "latest",
    "ember": "~1.7.0",
    "ember-data": "~1.0.0-beta.9",
    "emblem": "~0.3.18"
  },
  "exportsOverride": {
    "modernizr": {
      "js": "**/modernizr.js"
    },
    "foundation": {
      "js": "**/*.min.js",
      "css": "**/*.css"
    },
    "jquery": {
      "js": "dist/jquery.js"
    },
    "jquery.*": {
      "js": "**/*.js"
    },
    "*": {
      "js": "**/*.js",
      "css": "**/*.css"
    }
  }
}
